  "scripts": {
    "predev": "npm i && npm run migrate:all && npm run seed:all",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect --ignore-watch node_modules src/index.ts",
    "docker:start": "docker start nome_container && npm run dev",
    "docker:stop": "docker stop nome_container",
    "typeorm": "ts-node-dev  -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "build": "tsc",
    "start": "pm2 start ecosystem.config.js",
    "stop": "pm2 stop ecosystem.config.js",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --detectOpenHandles",
    "test:verbose": "jest --passWithNoTests --runInBand --detectOpenHandles",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:covarage": "npm test -- --coverage"
  },
